@model IEnumerable<VaccineManagement.Models.DataContext>


@{
    ViewData["Title"] = "BookedDetails";
}
<div class="row">
    <div class="col-md-10"><h3>Booked Details</h3></div>
    <div class="col-md-2">
        <div class="row" >
            <div style="padding:5px">
                <a class="nav-link text-dark" asp-area="" asp-controller="Vaccine" asp-action="Book" style="background-color:Highlight;border-radius:5px">Book</a>
            </div>
            <div style="padding:5px">
                <a class="nav-link text-dark" asp-area="" asp-controller="Vaccine" asp-action="Logout" style="background-color: indianred;border-radius: 5px">Logout</a>
            </div>
        </div>
        
    </div>
</div>


<div>
    <!--<h4>Booking</h4>-->
    <hr />
    <table class="table table-light" style="background-color:lightgrey">
        <tr style="background-color:darkslategrey;color:white">
            <th>ID</th>
            <th>Name</th>
            <th>Age</th>
            <th>Gender</th>
            <th>City</th>
            <th>Schedule</th>
            <th></th>
            <th></th>
            <th></th>
            
        </tr>
        @foreach (var item in ViewBag.details)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>
                <td>@item.age</td>
                <td>@item.Gender</td>
                <td>@item.city</td>
                <td>@item.Schedule</td>
                <td hidden>@item.AID</td>
                <td><a asp-action ="Update" asp-route-id="@item.Id" class="btn btn-danger">Update</a></td>
                <td><a asp-action ="Delete" asp-route-id="@item.Id" class="btn btn-danger">Delete</a></td>
            </tr>
        }
    </table>
